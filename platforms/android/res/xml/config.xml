<?xml version='1.0' encoding='utf-8'?>
<widget id="it.elmariachistudios.reforganizerui" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Compass">
        <param name="android-package" value="org.apache.cordova.deviceorientation.CompassListener" />
    </feature>
    <feature name="Sms">
        <param name="android-package" value="com.cordova.plugins.sms.Sms" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SpinnerDialog">
        <param name="android-package" value="hu.dpal.phonegap.plugins.SpinnerDialog" />
    </feature>
    <feature name="LaunchNavigator">
        <param name="android-package" value="uk.co.workingedge.phonegap.plugin.LaunchNavigator" />
    </feature>
    <name>RefOrganizerApp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access origin="*://*.googleapis.com/*" subdomains="true" />
    <access origin="*://*.gstatic.com/*" subdomains="true" />
    <access origin="*://*.google.com/*" subdomains="true" />
    <access origin="*://*.googleusercontent.com/*" subdomains="true" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <splash density="land-hdpi" src="platforms/android/res/drawable-land-hdpi/landhdpi.png" />
    <splash density="land-ldpi" src="platforms/android/res/drawable-land-ldpi/landldpi.png" />
    <splash density="land-mdpi" src="platforms/android/res/drawable-land-mdpi/landmdpi.png" />
    <splash density="land-xhdpi" src="platforms/android/res/drawable-land-xhdpi/landxhdpi.png" />
    <splash density="port-hdpi" src="platforms/android/res/drawable-port-hdpi/porthdpi.png" />
    <splash density="port-ldpi" src="platforms/android/res/drawable-port-ldpi/portldpi.png" />
    <splash density="port-mdpi" src="platforms/android/res/drawable-port-mdpi/portmdpi.png" />
    <splash density="port-xhdpi" src="platforms/android/res/drawable-port-xhdpi/portxhdpi.png" />
    <icon src="platforms/android/res/drawable/reforg.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="webView" value="org.crosswalk.engine.XWalkWebViewEngine" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="5000" />
</widget>
